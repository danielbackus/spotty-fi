{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["problems", "prettier"],
  "rules": {
    "no-param-reassign": [
      "error",
      {
        "props": true
      }
    ],
    "max-params": ["error", 3]
  }
}
